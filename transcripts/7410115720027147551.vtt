words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8699376947040498,"source_height":0.16165303738317757},{"text":"but don't worry Madison taught him all about it","start_time":50321,"end_time":52320,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.7850467289719626,"source_height":0.11828271028037383},{"text":"and now dad is as smart as a second grader","start_time":52321,"end_time":54462,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8995327102803738,"source_height":0.11828271028037383}]}