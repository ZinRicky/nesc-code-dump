{"utterances":[{"text":"let's make a Turkey for Thanksgiving","start_time":200,"end_time":1840,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8417777777777778,"source_height":0.128},{"text":"we're doing a dry brine which will penetrate the meat for two to three days","start_time":1841,"end_time":4840,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8826666666666667,"source_height":0.22700000000000004},{"text":"breaking down the muscle fibers and connective tissue","start_time":4841,"end_time":7040,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.7111111111111111,"source_height":0.17750000000000002},{"text":"which results in a tender","start_time":7041,"end_time":8080,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8711111111111112,"source_height":0.078},{"text":"more flavorful meat with a crispy skin","start_time":8081,"end_time":10120,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8951111111111112,"source_height":0.128},{"text":"after washing the thawed Turkey","start_time":10121,"end_time":11520,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.6684444444444445,"source_height":0.128},{"text":"I dry it with paper towels and rub my salt and herb brine all over my Turkey","start_time":11521,"end_time":15160,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.896,"source_height":0.17750000000000002},{"text":"inside the cavity the legs","start_time":15161,"end_time":16840,"words":null,"text_size":28,"text_color":"#FFFFFFFF","bg_color":"#00000000","alignment":0,"source_width":0.8853333333333333,"source_height":0.078},{"text":"wings breast","start_time":16841,"end_time":1